console.clear()
console.show

if (model == null || model.id == null)
    fatal("You must select a model or some Archimate elements.");
load(__SCRIPTS_DIR__ + 'lib/lib.ajs')

$('element').forEach(e => {
    embedProperties(e);
});

$('relationship').forEach(e => {
    embedProperties(e);
    if (e.name !== '')
        e.prop('Identifier', e.name)
    else
        e.removeProp('Identifier')
});
console.log('Done')