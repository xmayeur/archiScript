/**
 * Delete Unused Elements & relationships
 */

function deleteUnusedElements() {
    $("element").each(function (c) {
        if ($(c).objectRefs().isEmpty()) {
            c.delete();
        }
    });
    $('folder').find('folder').forEach(f => {
        if ($(f).find().not('folder').size() === 0)
            f.delete()
    })
}

deleteUnusedElements()