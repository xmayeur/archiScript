console.clear()
console.show

if ( model == null || model.id == null )
	fatal("You must select a model or some Archimate elements.");
load(__SCRIPTS_DIR__ + 'lib/lib.ajs')

$('element').forEach( e => {
    expandProperties(e);
});

$('relationship').forEach( e => {
        if ('Identifier' in e.prop() &&  e.prop('Identifier') !== '')
             e.documentation = e.prop('Identifier')
});

console.log('Done')